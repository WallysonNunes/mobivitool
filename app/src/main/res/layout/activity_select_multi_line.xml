<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="wallyson.lima.mobivitool.view.SelectMultiLineActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">

        <Spinner
            android:id="@+id/spinPrefixo1Multi"
            android:layout_width="match_parent"
            android:layout_height="60dp" />

        <Spinner
            android:id="@+id/spinPrefixo2Multi"
            android:layout_width="match_parent"
            android:layout_height="60dp" />

        <Spinner
            android:id="@+id/spinPrefixo3Multi"
            android:layout_width="match_parent"
            android:layout_height="60dp" />

        <Spinner
            android:id="@+id/spinAno1Multi"
            android:layout_width="match_parent"
            android:layout_height="60dp" />

        <Spinner
            android:id="@+id/spinAno2Multi"
            android:layout_width="match_parent"
            android:layout_height="60dp" />

        <Spinner
            android:id="@+id/spinAno3Multi"
            android:layout_width="match_parent"
            android:layout_height="60dp" />

        <Button
            android:id="@+id/btSelecionarMulti"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="80dp"
            android:layout_marginTop="80dp"
            android:text="Selecionar" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
